#include <stdio.h>

int main() {


 printf("\n--Finding area of a sqaure--" );
            //area of sqare = side * side
           
            float s, area;
            printf("\nEnter length of side of square: ");
            scanf("%f", &s);
            area = s*s;

            printf("Area of square = %.2f", area);
            
            
}
